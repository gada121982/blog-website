
        <%- include('../layout/header.admin.ejs') -%>
        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">Category
                            <small>List</small>
                        </h1>
                    </div>
                    <!-- /.col-lg-12 -->
                    <% if(succesful === 1){%>
                        <div class="alert alert-success" role="alert">Delete succesful </div>
                     <% }%>  
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr align="center">
                                <th>ID</th>
                                <th>Tên</th>
                                <th>Tên không dấu</th>
                                <th>Ngày tạo</th>
                                <th>Delete</th>
                                <th>Edit</th>   
                            </tr>
                        </thead>
                        <tbody>
                            <%# ! for loop add content %>
                           
                                <% dstheloai.forEach(function(ds){ %>
                                <tr class="odd gradeX" align="center">
                                    <td><%= ds._id %></td>
                                    <td><%= ds.ten %></td>
                                    <td><%= ds.tenkhongdau %></td>
                                    <td><%= ds.createAt %></td>
                                    <td class="center"><i class="fa fa-trash-o  fa-fw"></i><a href="admin/theloai/delete/<%= ds._id%>"> Delete</a></td>
                                    <td class="center"><i class="fa fa-pencil fa-fw"></i> <a href="admin/theloai/edit/<%= ds._id%>">Edit</a></td>
                                </tr>
                                <% });
                                 %>
                           

                        </tbody>
                    </table>
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
    
     
        <%- include('../layout/footer.admin.ejs') -%>