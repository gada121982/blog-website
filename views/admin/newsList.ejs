
      <%- include('../layout/header.admin.ejs') -%>
      <!-- Page Content -->
      <div id="page-wrapper">
          <div class="container-fluid">
              <div class="row">
                  <div class="col-lg-12">
                      <h1 class="page-header">Loại tin
                          <small>List</small>
                      </h1>
                  </div>
                  <!-- /.col-lg-12 -->
                  <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                      <thead>
                          <tr align="center">
                              <th>ID</th>
                              <th>Tiêu đề</th>
                              <th>Description</th>
                              <th>Tên không dấu</th>
                              <th>Loại tin</th>
                              <th>Content</th>
                              <th>Views</th>
                              <th>Comment</th>
                              <th>Like</th>
                              <th>Nổi bật</th>
                              <th>Update at</th>
                              <th>Delete</th>
                              <th>Edit</th>
                          </tr>
                      </thead>
                      <tbody>
                          <% 
                              dstintuc.forEach(element => {%>
                                  <tr class="odd gradeX" align="center">
                                      <td><%= element._id %> </td>
                                      <td><%= element.tieude %></td>    
                                      <td><%= element.description%></td>
                                      <td><%= element.tieudekhongdau %></td>
                                      <td><%= element.loaitin %></td>
                                      <td>
                                        <img src="<%= element.image%>" style="height : 50px ; width : 50px"alt="">
                                      
                                      <td><%= element.countview %></td>
                                      <td><%= element.countcomment %></td>
                                      <td><%= element.countlike %></td>
                                      <td><%= element.important %></td>
                                      <td><%= element.updateat %></td>
                                      <td class="center"><i class="fa fa-trash-o  fa-fw"></i><a href="admin/tintuc/delete/<%= element._id%>"> Delete</a></td>
                                      <td class="center"><i class="fa fa-pencil fa-fw"></i> <a href="admin/tintuc/edit/<%= element._id%>">Edit</a></td>
                                  </tr>
                              <%});
                          %>
                      
                      
                      </tbody>
                  </table>
              </div>
              <!-- /.row -->
          </div>
          <!-- /.container-fluid -->
      </div>
      <!-- /#page-wrapper -->
<%- include('../layout/footer.admin.ejs') -%>