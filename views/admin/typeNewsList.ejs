
      <%- include('../layout/header.admin.ejs') -%>
            <!-- Page Content -->
            <div id="page-wrapper">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <h1 class="page-header">Loại tin
                                <small>List</small>
                            </h1>
                        </div>
                        <!-- /.col-lg-12 -->
                        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                                <tr align="center">
                                    <th>ID</th>
                                    <th>Tên thể loại</th>
                                    <th>Tên</th>
                                    <th>Tên không dấu</th>
                                    <th>Create At</th>
                                    <th>Update At</th>
                                    <th>Delete</th>
                                    <th>Edit</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% 
                                    dsloaitin.forEach(element => {%>
                                        <tr class="odd gradeX" align="center">
                                            <td><%= element._id %> </td>
                                            <td><%= element.tentheloai %></td>
                                            <td><%= element.tenloaitin %></td>
                                            <td><%= element.tenkhongdau %></td>
                                            <td><%= element.createat %></td>
                                            <td><%= element.updateat %></td>
                                            
                                            <td class="center"><i class="fa fa-trash-o  fa-fw"></i><a href="admin/loaitin/delete/<%= element._id%>"> Delete</a></td>
                                            <td class="center"><i class="fa fa-pencil fa-fw"></i> <a href="admin/loaitin/edit/<%= element._id%>">Edit</a></td>
                                        </tr>
                                    <%});
                                %>
                            
                            
                            </tbody>
                        </table>
                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.container-fluid -->
            </div>
            <!-- /#page-wrapper -->
      <%- include('../layout/footer.admin.ejs') -%>